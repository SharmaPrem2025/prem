			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSynapseAnalytics2'"
		},
		"mydatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'mydatabase'"
		},
		"mydatabaseconnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'mydatabaseconnection'"
		},
		"mysourceblob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'mysourceblob'"
		},
		"mysourcetest_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'mysourcetest'"
